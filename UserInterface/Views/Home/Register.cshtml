@model UserRegisterViewModel
@{
    Layout = null;
}
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Admin</title>
    <link href="~/twitter-bootstrap/css/bootstrap.min.css" rel="stylesheet" />
</head>
<body>
    <form class="w-50 mx-auto mt-5 shadow" asp-action="Register2" method="post">
        <input type="text" asp-for="Name" class="form-control" name="Name" value="" />
        <span asp-validation-for="Name" class="text-danger"></span>
        <button type="submit" class="my-2">Send</button>
    </form>
    <script src="~/jquery/jquery.min.js"></script>
    <script src="~/jqueryValidate/jquery.validate.min.js"></script>
    <script src="~/jqueryUnob/jquery.validate.unobtrusive.min.js"></script>
</body>
</html>